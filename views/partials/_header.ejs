<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
   
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav"></div>
          <a class="nav-item nav-link" href="/urls">My URLs</a>
          <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      <!-- login form -->
      <% if (!user) { %>
       <button class="btn btn-outline-dark my-2 my-sm-0"><a class="nav-item nav-link" href="/login">Login</a></button>
       <button class="btn btn-outline-dark my-2 my-sm-0"><a class="nav-item nav-link" href="/register">Register</a></button>
      <!-- logout form -->
      <% } else { %>
        <form class = "form-inline" action ="/logout", method = "POST">
        <button class="btn btn-outline-dark my-2 my-sm-0" type="submit">Logout</button>
        </form>
      <% }%>
      </div>
    </div>
  </nav>
</header>
